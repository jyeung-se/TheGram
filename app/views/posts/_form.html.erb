<center>

<br>
<%= form_for @post, html: { multipart: true } do |f| %>

  <%= f.label :picture %>
  <%= f.file_field :picture %><br><br>

  <%= f.label :caption %>
  <%= f.text_area :caption %><br><br>

  <%= f.label :user_id %>
  <%= f.hidden_field :user_id, value: current_user.id %>

  <%= f.submit %>

<% end %>

<%= link_to "All users", users_path %>
<%= link_to "All posts", posts_path %>



</center>
